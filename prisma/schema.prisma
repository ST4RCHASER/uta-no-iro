// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = env("DATABASE_URL")
}

model SongQueue {
    id        Int      @id @default(autoincrement())
    order     Int
    data      String
    createdAt DateTime @default(now())
    playedAt  DateTime?
    deletedAt DateTime?
}

model KV {
    id        Int      @id @default(autoincrement())
    key       String   @unique
    value     String
    createdAt DateTime @default(now())
    deletedAt DateTime?
}