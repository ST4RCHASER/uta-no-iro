{
    "extends": "./tsconfig.json",
    "compilerOptions": {
        "module": "Preserve",
        "outDir": "dist",
        "target": "ESNext",
        "lib": [
            "ESNext",
            "DOM"
        ],
        "isolatedModules": false,
        "noEmit": false
    },
    "include": [
        "next-env.d.ts",
        "src/**/*.ts",
        "src/**/*.tsx"
    ]
}